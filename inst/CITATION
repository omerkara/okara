citHeader("To cite okara in publications use:")

citEntry(entry = "Manual",
  title        = "Omer Kara's Personal Utility Functions",
  author       = personList(as.person("Omer Kara")),
  journal      = "",
  year         = "",
  volume       = "",
  number       = "",
  pages        = "",
  url          = "https://github.com/omerkara/okara",

  textVersion  =
  paste("Omer Kara (2023).",
        "Omer Kara's Personal Utility Functions.",
        "URL https://github.com/omerkara/okara.")
)
