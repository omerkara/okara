citHeader("To cite okara in publications use:")

citEntry(entry = "Manual",
  title        = "Ömer Kara's Personal Utility Functions",
  author       = personList(as.person("Ömer Kara")),
  journal      = "",
  year         = "",
  volume       = "",
  number       = "",
  pages        = "",
  url          = "https://github.com/omerkara/okara",

  textVersion  =
  paste("Ömer Kara (2021).",
        "Ömer Kara's Personal Utility Functions.",
        "URL https://github.com/omerkara/okara.")
)
